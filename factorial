import java.util.*;
public class Main{
    static void factorial(int a,int fact){
        for(int i=1;i<=a;i++){
            fact=fact*i;
        }
        System.out.println(fact);
    }
    public static void main(String...args){
        Scanner g=new Scanner(System.in);
        int a=g.nextInt();
        int fact=1;
        factorial(a,fact);
    }
}
