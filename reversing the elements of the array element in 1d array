import java.util.*;
public class rahul{
    public static void main(String...args){
        Scanner g=new Scanner(System.in);
        int a=g.nextInt();
        int arr[]=new int[a];
        int brr[]=new int[a];
        for(int i=0;i<a;i++){
            int b=g.nextInt();
            arr[i]=b;
        }
        
        for(int j=a-1;j>=0;j--){
            brr[(a-1)-j]=arr[j];
        }
        for(int i=0;i<a;i++){
            System.out.print(brr[i]+" ");
        }
            
    }
}
