import java.util.*;
public class Rahul{
    public int rahul(int a,int i){
        int c=a%i;
        return c;
       
    }
    public static void main(String...args){
        Rahul obj=new Rahul();
        Scanner d=new Scanner(System.in);
        int a=d.nextInt();
        System.out.println("The factors of " + a);
        
        for(int i=1;i<=a;i++){
            int result=obj.rahul(a,i);
            if(result==0){
                System.out.println(i);
            }
        }
        
        
        
    }
}
