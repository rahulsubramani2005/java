import java.util.*;
public class Main{
    static void fib(int a){
        int b=0;
        int c=1;
        int sum=0;
        System.out.print(b);
        for(int i=1;i<a;i++){
            System.out.print(c);
            sum=b+c;
          
            b=c;
            c=sum;
            
        }
    }
        
    public static void main(String...args){
        Scanner g=new Scanner(System.in);
        int a=g.nextInt();
        int sum=0;
        fib(a);
    }
}
