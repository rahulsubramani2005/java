import java.util.*;

public class Main {
    public static void main(String... args) {
        Scanner g = new Scanner(System.in);
        int a[] = {1, 2, 3, 4, 5};
        int size = a.length;
       
        

        for (int i = 1; i < size-1; i++) {
            a[i] = a[i - 1] * a[i + 1];
            
            
            
            
        }
        a[0]=3;
        a[4]=60;
        System.out.print(Arrays.toString(a));
        
        
    }
}
