import java.util.*;
public class Main
{
    public int rahul(int a){
        int c=0;
        c=a+1;
        return c;
        
            
        
    }
    public static void main(String...args){
        Scanner g=new Scanner(System.in);
        int a=g.nextInt();
        Main obj=new Main();
        int result=obj.rahul(a);
        System.out.println(result);
    }
}
