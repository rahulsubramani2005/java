import java.util.*;

public class Main {
    public static void main(String... args) {
        Scanner a = new Scanner(System.in);
        int r=a.nextInt();
        int c=a.nextInt();
        for(int i=1;i<=r;i++){
            for(int j=1;j<=c;j++){
                System.out.println("*");
            }
            System.out.println();
        }
    }
}
