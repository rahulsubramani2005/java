import java.util.*;
public class Main{
    public static void main(String...args){
        Scanner a=new Scanner(System.in);
        int b=a.nextInt();
        char d='A';
        for(int i=1;i<=b;i++){
            for(int j=1;j<=i;j++){
                System.out.print(d);
            }
            System.out.println();
            d++;
        }
        }
    }
