import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner a= new Scanner(System.in);
	    int b=a.nextInt();
	    int c=0;
	    int rev=0;
	    
	    while(b!=0){
	        c=b%10;
	        rev=rev*10+c;
	        b=b/10;
	    }
	    if(b==rev){
	            System.out.println("Palindrome");
	        }
	        else{
	            System.out.println("Not Palindrome");
	        }
	    }
	    
}
