import java.util.*;

public class Main {
    public static void main(String... args) {
        Scanner a = new Scanner(System.in);
    
        char A[] = {'r', 'a', 'h', 'u', 'l'};
        int b = A.length;
        String c="";
        for(int i=b-1;i>=0;i--){
            c=c+A[i];
        }
        System.out.println(c);
        
    }
}
