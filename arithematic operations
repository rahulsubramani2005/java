
 public class Main {
    int a=10,b=4;
    public static void main(String... args) {
        Main f =new Main();
        
        System.out.println(f.a+f.b);
        System.out.println(f.a-f.b);
        System.out.println(f.a*f.b);
        System.out.println(f.a/f.b);
        System.out.println(f.a%f.b);
    } 
     
 }
