class Animal{
    public void makesound(){
        System.out.println("Animal Sound");
    }
}
class Dog extends Animal{
    public void makesound(){
        System.out.println("Dog Barks");
    }
}
class Cat extends Animal{
    public void makesound(){
        System.out.println("Cat Meows");
    }
}
public class Main{
    public static void main(String...args){
        Animal a=new Animal();
        Dog d=new Dog();
        Cat c=new Cat();
        a.makesound();
        d.makesound();
        c.makesound();
        
    }
}
