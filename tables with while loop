import java.util.*;

public class Main {
    public static void main(String... args) {
        Scanner b = new Scanner(System.in);
        int i=0;
        int a = b.nextInt(); 
        while(i<11) {
            
            System.out.println(a + " * " + i + " = " + (a * i));
            i=i+1;
        }
    }
}
